{
  "expo": {
    "name": "FitCommit",
    "slug": "FitCommit",
    "version": "1.0.2",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "resizeMode": "contain",
      "backgroundColor": "#6B73FF"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.mmur3.FitCommitMobileApp",
      "buildNumber": "4",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.mmur3.FitCommitMobileApp"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-font"
    ],
    "extra": {
      "eas": {
        "projectId": "701e745a-04a0-4644-8cb2-861dcbac0ee5"
      }
    },
    "owner": "mmur3",
    "scheme": "fitcommit"
  }
}
